<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity"
    android:background="@drawable/background">

    <!-- Dodanie ComposeView do istniejącego layoutu -->

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="424dp"
        android:layout_height="891dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="32dp"

        android:layout_marginEnd="42dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="988dp">

            <FrameLayout
                android:id="@+id/frameLayout"
                android:layout_width="357dp"
                android:layout_height="231dp"
                android:background="@drawable/background"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.07999998">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <FrameLayout
                        android:id="@+id/frameLayout4"
                        android:layout_width="123dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/roundedcorners"
                        android:padding="2dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <EditText
                            android:id="@+id/kilomiters"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:ems="10"
                            android:hint="@string/kilomiters1"
                            android:inputType="number"
                            android:textAlignment="center" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout5"
                        android:layout_width="123dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="4dp"
                        android:background="@drawable/roundedcorners"
                        android:padding="2dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.1"
                        app:layout_constraintStart_toEndOf="@+id/litry"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout4">

                        <EditText
                            android:id="@+id/fuel"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:ems="10"
                            android:hint="@string/fuel1"
                            android:inputType="number"
                            android:textAlignment="center"
                            android:textColor="#3A3A3A" />

                    </FrameLayout>


                    <FrameLayout
                        android:id="@+id/frameLayout3"
                        android:layout_width="123dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="4dp"
                        android:background="@drawable/roundedcorners"
                        android:padding="2dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.928"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout5">


                        <EditText
                            android:id="@+id/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="Koszt paliwa"
                            android:inputType="number" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout2"
                        android:layout_width="123dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="16dp"
                        android:background="@drawable/roundedcorners"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.93"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout3"
                        app:layout_constraintVertical_bias="0.49">


                        <EditText
                            android:id="@+id/date"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:ems="10"
                            android:hint="@string/dat1e"
                            android:inputType="datetime"
                            android:textAlignment="center"
                            android:textColor="@color/black" />

                    </FrameLayout>

                    <TextView
                        android:id="@+id/kilometry"
                        android:layout_width="166dp"
                        android:layout_height="44dp"
                        android:layout_marginTop="12dp"
                        android:background="@drawable/roundedcorners2"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="@string/kilomiters1"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toTopOf="@+id/litry"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.078"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.59000003" />

                    <TextView
                        android:id="@+id/litry"
                        android:layout_width="165dp"
                        android:layout_height="37dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="32dp"
                        android:layout_marginBottom="21dp"
                        android:background="@drawable/roundedcorners2"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="@string/fuel1"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toTopOf="@+id/koszt"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.045"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/kilometry"
                        app:layout_constraintVertical_bias="0.53" />

                    <TextView
                        android:id="@+id/koszt"
                        android:layout_width="165dp"
                        android:layout_height="37dp"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/roundedcorners2"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="@string/money1"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.077"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/litry" />

                    <TextView
                        android:id="@+id/data"
                        android:layout_width="165dp"
                        android:layout_height="37dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="13dp"
                        android:background="@drawable/roundedcorners2"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="@string/dat1e"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.077"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/koszt"
                        app:layout_constraintVertical_bias="0.53" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </FrameLayout>

            <com.jjoe64.graphview.GraphView
                android:id="@+id/idGraphView"
                android:layout_width="362dp"
                android:layout_height="258dp"
                android:layout_alignParentTop="true"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.326"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout6"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="122dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="124dp"
                android:text="Spalanie Samochodu"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/confirm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="224dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:text="potwierdz"
                app:layout_constraintBottom_toTopOf="@+id/frameLayout6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.49"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

            <FrameLayout
                android:id="@+id/frameLayout6"
                android:layout_width="360dp"
                android:layout_height="173dp"
                android:layout_marginTop="56dp"
                android:background="@drawable/background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <FrameLayout
                        android:id="@+id/frameLayout7"
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.085"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/spalanie1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="Spalanie Samochodu"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout8"
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="12dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.091"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout7">

                        <TextView
                            android:id="@+id/kosztCalkowity"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="Koszt Paliwa"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout11"
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="12dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.09"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout8"
                        app:layout_constraintVertical_bias="0.0">

                        <TextView
                            android:id="@+id/odleglosc"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="Przejechana odległość"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout9"
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="2dp"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.157"
                        app:layout_constraintStart_toEndOf="@+id/frameLayout7"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/spalanieDane"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="TextView"
                            android:textColor="@color/text" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout10"
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginTop="12dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintStart_toEndOf="@+id/frameLayout8"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout9">

                        <TextView
                            android:id="@+id/kosztCalkowityDane"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="TextView"
                            android:textColor="@color/text" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="160dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:background="@drawable/roundedcorners2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.56"
                        app:layout_constraintStart_toEndOf="@+id/frameLayout11"
                        app:layout_constraintTop_toBottomOf="@+id/frameLayout10"
                        app:layout_constraintVertical_bias="0.0">

                        <TextView
                            android:id="@+id/odlegloscDane"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="TextView"
                            android:textColor="@color/text" />
                    </FrameLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </FrameLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
